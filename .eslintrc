{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "babel/generator-star-spacing": 1,
    "babel/new-cap": 1,
    "babel/array-bracket-spacing": 1,
    "babel/object-shorthand": 1,
    "babel/arrow-parens": 1,
    "babel/no-await-in-loop": 1,
    "babel/flow-object-type": 1
  },
  "plugins": [
    "babel"
  ]
}